# Git Functions


# Git Aliases
alias gs='git status'
alias ga='git add'
alias gc='git commit'
alias gpull='git pull'
alias gpush='git push'
alias glog='git log --oneline --decorate --color'
alias gammend='git commit --amend --no-edit'
alias gundo='git reset --soft HEAD~1'
alias gnuke='git clean -df; git reset --hard'
