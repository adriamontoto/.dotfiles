  "todo-tree.tree.scanMode": "workspace only",
  "todo-tree.general.showActivityBarBadge": true,
  "todo-tree.general.showIconsInsteadOfTagsInStatusBar": true,
  "todo-tree.general.tags": ["todo:", "TODO:"],
  "todo-tree.highlights.enabled": false,
  "todo-tree.highlights.foregroundColourScheme": [],
  "todo-tree.tree.disableCompactFolders": true,
  "todo-tree.tree.buttons.expand": false,
  "todo-tree.tree.showCurrentScanMode": false,
  "todo-tree.tree.hideTreeWhenEmpty": true,
  "todo-tree.filtering.excludeGlobs": ["**/node_modules/*/**", "**/*env*/**"],

  "python.globalModuleInstallation": true,
  "python.analysis.autoFormatStrings": true,
  "python.analysis.inlayHints.functionReturnTypes": true,
  "python.analysis.inlayHints.pytestParameters": true,
  "python.analysis.typeCheckingMode": "basic",
  "python.analysis.autoImportCompletions": true,
  "python.analysis.diagnosticSeverityOverrides": {
    "reportGeneralTypeIssues": "none",
    "reportUnknownVariableType": "none",
    "reportUnusedImport": "none"
  },
  "python.testing.pytestEnabled": true,
  "python.testing.unittestArgs": [],
  "python.languageServer": "Pylance",
  "python.terminal.activateEnvInCurrentTerminal": true,

  "pythonIndent.trimLinesWithOnlyWhitespace": true,
  "pythonIndent.useTabOnHangingIndent": true,
  "[python]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.formatOnPaste": true,
    "editor.formatOnSaveMode": "file",
    "editor.defaultFormatter": "eeyore.yapf",
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit"
    }
  },

  "autoDocstring.startOnNewLine": true,

  "notebook.formatOnCellExecution": true,
  "jupyter.askForKernelRestart": false,
  "isort.args": [
    "--force-alphabetical-sort-within-sections",
    "--atomic",
    "--line-length=120",
    "--thirdparty=pytest",
    "--thirdparty=uuid",
    "--project=backend",
    "--project=src",
    "--project=app",
    "--project=database",
    "--project=settings",
    "--project=ai",
    "--project=client",
    "--project=chat",
    "--project=calendar",
    "--project=action"
  ],

  "yapf.importStrategy": "useBundled",
  "yapf.args": ["--parallel", "--style={based_on_style: google, column_limit: 120, indent_width: 4}"],

  "mypy-type-checker.args": ["--ignore-imports"],

  "flake8.args": ["--max-line-length=120", "--max-doc-length=120", "--ignore=E124,E125"],

  "database-client.showUser": true,

  "yaml.format.singleQuote": true,
  "yaml.customTags": [
    "!And",
    "!And sequence",
    "!If",
    "!If sequence",
    "!Not",
    "!Not sequence",
    "!Equals",
    "!Equals sequence",
    "!Or",
    "!Or sequence",
    "!FindInMap",
    "!FindInMap sequence",
    "!Base64",
    "!Join",
    "!Join sequence",
    "!Cidr",
    "!Ref",
    "!Sub",
    "!Sub sequence",
    "!GetAtt",
    "!GetAZs",
    "!ImportValue",
    "!ImportValue sequence",
    "!Select",
    "!Select sequence",
    "!Split",
    "!Split sequence"
  ],

  "material-icon-theme.hidesExplorerArrows": true,
  "material-icon-theme.files.associations": {
    "requirements_dev.txt": "python-misc",
    "constraints.txt": "python-misc",
    "constraints_dev.txt": "python-misc",
    "**.mwb": "database"
  },
  "material-icon-theme.folders.associations": {
    "dal": "database",
    "exceptions": "error",
    "infrastructure": "Server"
  },

  "hediet.vscode-drawio.resizeImages": null,

  "sqliteViewer.maxFileSize": 0,

  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.autoIndent": "advanced",
    "editor.tabSize": 4
  },
  "[dockercompose]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.autoIndent": "advanced",
    "editor.tabSize": 4
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.autoIndent": "advanced",
    "editor.tabSize": 4
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.autoIndent": "advanced",
    "editor.tabSize": 4
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.autoIndent": "advanced",
    "editor.tabSize": 4
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.autoIndent": "advanced",
    "editor.tabSize": 4
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.autoIndent": "advanced",
    "editor.tabSize": 4
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.autoIndent": "advanced",
    "editor.tabSize": 4
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.autoIndent": "advanced",
    "editor.tabSize": 4
  },
  "prettier.printWidth": 120,
  "prettier.tabWidth": 2,
  "prettier.singleQuote": true,
  "prettier.semi": false,
  "prettier.endOfLine": "auto",

  "dotenv.enableAutocloaking": false,
  "editor.accessibilitySupport": "off",

  "aws.telemetry": false,
  "aws.resources.enabledResources": [],
  "aws.lambda.recentlyUploaded": {
    "profile:adria": {
      "eu-south-2": "test"
    }
  },

  "errorLens.followCursor": "closestProblem",
  "errorLens.messageTemplate": "$message -> '$source'",
  "errorLens.exclude": ["todo|TODO|bug|BUG|error|ERROR|fix|FIX|fixme|FIXME|note|NOTE|question|QUESTION|query|QUERY"]
}
